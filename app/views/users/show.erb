<h1>Your posts</h1>

<a href="/posts/new">Create Post</a>
<a href="/logout">Logout</a>
<p>Welcome, <%= current_user.username %></p>

<% @user.posts.each do |post| %>
  <%= post.name %>
  <%= post.ingredients %>
  <%= post.directions %>
  <%= post.cooktime %>
  <%= post.chef %>
  <%= post.caption %>
<% end %>
